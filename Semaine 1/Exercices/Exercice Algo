	Fin


// Exercice 5.8
Variable N,i,gn en Numerique
Debut
gn ← 0
Pour i ← 1 à 20
	Ecrire "Entrez un numéros"
	Lire N
	Si i = 1 ou n > gn Alors
	gn ← N
	FinSi
i Suivant
Ecrire " Le nombre le plus grand était :",gn
Fin

Variable N,i,gn,pos en Numerique
Début
gn ← 0
Pour i ← 1 à 20
Ecrire "Entrez votre nombre"
Lire N
Si i = 1 ou n > gn Alors
gn ← N
pos ← i
FinSi
i suivant
Ecrire " Le nombre le plus grand était :", gn
Ecrire " La position était la :", pos
Fin


// Exercice 5.9

Variable N,i,gn,pos en Numerique
Début
N ← 1
i ← 0
gn ← 0 
TantQue N <> 0
	Ecrire "Entrez votre nombre"
	ire N
	i ← i + 1
		Si i = 1 ou n > gn Alors
		gn ← N
		pos ← i
		FinSi
	FinTantQue
Ecrire " Le nombre le plus grand était :", gn
Ecrire " La position était la :", pos
Fin



// Exercice 5.10

Variable E,somdue,M,Reste,nb5e,nb10e, en Numerique
Début
E ← 1
somdue ← 0
	TantQue E <> 0
	Ecrire "Entrez votre montant :"
	Lire E
	somdue ← somdue + E
	FinTantQue
Ecrire "Vous devez :", somdue, "euros"
Ecrire "Montant versé"
Lire M
Reste ← M - somdue
nb10e ← 0
	TantQue Reste >= 10
	nb10e ← nb10e +1
	Reste ← Reste - 10
	FinTantQue
nb5e ← 0
	Si Reste >= 5
	nb5e ← 1
	Reste ← Reste -5
	FinSi
Ecrire "Rendu de la monaie :"
Ecrire "Billets de 10 E :",nb10e
Ecrire "Billets de 5 E :",nb5e
Ecrire "¨Pièce de 1 E :",Reste
Fin



// Exercice 5.11
Variable chance,nbc,cp,cj, i en Numerique
Debut
	Ecrire "Entrez le nombre de chevaux partants"
	Lire cp
	Ecrire "Entrez le nombre de chevaux joués"
	Lire cj
	nbc ← 1
	chance ← 1
	Pour i ← 1 à cj
	nbc ← nbc * (i+cp-cj)
	chance ← Chance * i
	I suivant
	Ecrire "Dans l'ordre une chance sur :",nbc
	Ecrire "Dans le désordre, une chance sur :",nbc/chance
	Fin
